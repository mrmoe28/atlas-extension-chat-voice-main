{
  "version": 2,
  "builds": [
    {
      "src": "dev/server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dev/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "dev/server/server.js"
    }
  ],
  "env": {
    "OPENAI_REALTIME_MODEL": "gpt-4o-realtime-preview"
  }
}
